{ "name": "crs",
  "version": "0.0.0",
  "main": "src",
  "private": true,
  "scripts": {
    "start": "node .",
    "test": "jest",
    "coverage": "npm test -- --coverage",
    "postcoverage": "opn coverage/lcov-report/index.html",
    "dev": "nodemon -i \"*.test.js\" .",
    "prod": "cross-env NODE_ENV=production nodemon -i \"*.test.js\" -r dotenv-safe/config .",
    "lint": "eslint src",
    "docs": "apidoc -i src -o docs && apidoc-markdown -p docs -o DOCS.md",
    "postdocs": "opn docs/index.html",
    "clean": "rm -rf build && mkdir build",
    "build": "babel src -d dist --ignore \"src/**/*.test.js\""
  },
  "jest": {
    "testEnvironment": "node",
    "setupTestFrameworkScriptFile": "<rootDir>/test/setup.js"
  },
  "dependencies": {
    "@babel/cli": "^7.21.5",
    "@babel/core": "^7.21.8",
    "@babel/plugin-proposal-export-default-from": "^7.18.10",
    "@babel/plugin-transform-runtime": "^7.21.4",
    "@babel/preset-env": "^7.21.5",
    "@babel/register": "^7.21.0",
    "@babel/runtime": "^7.21.5",
    "apidoc": "^1.0.2",
    "apidoc-markdown": "^7.2.5",
    "babel-eslint": "^10.1.0",
    "cross-env": "^7.0.3",
    "dotenv-safe": "^8.2.0",
    "eslint": "^8.39.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-standard": "^5.0.0",
    "jest-cli": "^29.5.0",
    "mongodb-memory-server": "^8.12.2",
    "nock": "^13.3.1",
    "nodemon": "^2.0.22",
    "opn-cli": "^5.0.0",
    "sinon": "^15.0.4",
    "supertest": "^6.3.3"
  },
  "devDependencies": {
    "@sendgrid/mail": "^7.7.0",
    "bcryptjs": "^2.4.3",
    "bluebird": "^3.7.2",
    "body-parser": "^1.20.2",
    "bodymen": "^1.1.1",
    "check-types": "^11.2.2",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "csvtojson": "^2.0.10",
    "date-fns": "^2.30.0",
    "eslint-plugin-prettier": "^4.2.1",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.0",
    "mongoose": "^7.1.0",
    "mongoose-create-unique": "^0.4.4",
    "mongoose-keywords": "^0.4.1",
    "morgan": "^1.10.0",
    "passport": "^0.6.0",
    "passport-http": "^0.3.0",
    "passport-http-bearer": "^1.0.1",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "prettier": "^2.8.8",
    "querymen": "^2.1.4",
    "rand-token": "^1.0.1",
    "request": "^2.88.2",
    "request-promise": "^4.2.6",
    "type-of-is": "^3.5.1",
    "winston": "^3.8.2"
  }
}
